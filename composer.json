{
  "name": "makaira/shopware6-connect",
  "description": "Connect Shopware 6 to Makaira",
  "type": "shopware-platform-plugin",
  "license": "proprietary",
  "version": "0.1.0",
  "require": {
    "php": ">= 8.2",
    "shopware/core": "6.5.*",
    "league/pipeline": "^1.0",
    "phpdocumentor/reflection-docblock": "^5.0",
    "psr/clock": "^1.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.34",
    "psalm/plugin-symfony": "^5.0",
    "vimeo/psalm": "^5.15",
    "rector/rector": "^1.2",
    "frosh/shopware-rector": "^0.4"
  },
  "autoload": {
    "psr-4": {
      "Makaira\\Connect\\": "src/"
    }
  },
  "extra": {
    "shopware-plugin-class": "Makaira\\Connect\\MakairaConnect",
    "author": "Makaira GmbH",
    "label": {
      "de-DE": "Makaira Connect",
      "en-GB": "Makaira Connect"
    }
  },
  "config": {
    "allow-plugins": {
      "symfony/runtime": true
    }
  }
}
